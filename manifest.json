{
  "name": "Justin/Twitch tv online",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Check if someone is streaming on twitch or justin",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["jquery.js","background.js"]
  },
  "permissions": [
    "http://api.justin.tv/","background","storage"
  ]
}
